
.container {
	position: absolute;
	width: 100%;
	height: 100%;
}
.title{
	position: absolute;
	margin-top: 50rpx;
	margin-left: 50rpx;
	width: 100rpx;
	height: 100rpx;
}
.daily_right {
	position: absolute;
	width: 750rpx;
	height: 600rpx;
}
@-webkit-keyframes shake {
0% {
		 opacity:1;
		-webkit-transform: scale(0.6);
}
100% {
		 opacity:0;
		-webkit-transform: scale(1);
}
}
@keyframes shake {
0% {
		 opacity:1;
		-webkit-transform: scale(0.6);
}
100% {
		 opacity:0;
		-webkit-transform: scale(1);
}
}
.daily_right view {
	position: absolute;
	width: 600rpx;
	height: 600rpx;
	margin-left: 75rpx;
	/*move left 100px*/
	margin-top: 200rpx;
	border-radius: 300rpx;
	background-color: transparent;
	border-color: #FFFFFF;
	border-style: solid;
	border-width: 2rpx;
}
.shake1 {
	-webkit-animation: shake 5s 0s infinite;
	        animation: shake 5s 0s infinite;
	-webkit-animation-timing-function: ease;
	        animation-timing-function: ease;
}
.shake2 {
	-webkit-animation: shake 6s 2s infinite;
	        animation: shake 6s 2s infinite;
	-webkit-animation-timing-function: ease;
	        animation-timing-function: ease;
}
.shake3 {
	-webkit-animation: shake 8s 3s infinite;
	        animation: shake 8s 3s infinite;
	-webkit-animation-timing-function: ease;
	        animation-timing-function: ease;
}
