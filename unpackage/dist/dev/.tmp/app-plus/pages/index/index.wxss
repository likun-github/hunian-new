
.container {
	position: absolute;
	width: 100%;
	height: 100%;
}
.title {
	font-size: 40rpx;
	color: #ffffff;
	position: relative;
	margin-top: 50rpx;
	margin-left: 50rpx;
}
.title1 {
	font-size: 24rpx;
	color: #ffffff;
	position: relative;
	margin-top: 30rpx;
	margin-left: 50rpx;
}
.cicle {
	position: relative;
	margin-top: 700rpx;
	margin-left: 50rpx;
	width: 200rpx;
	border-radius: 50rpx;
	height: 80rpx;
	background-color: #eeeeee;
}
.cicle .triangle {
	position: absolute;
	margin-left: 40rpx;
	margin-top: 25rpx;
	width: 0;
	height: 0;
	border: 18rpx solid transparent;
	border-left: 20rpx solid rgb(200, 200, 200);
}
.line {
	position: relative;
	width: 700rpx;
	margin-top: 50rpx;
	margin-left: 25rpx;
	height: 2rpx;
	background-color: #dddddd;
}
.scroll-view_H {
	position: relative;
	overflow: hidden;
	margin-top: 20rpx;
}
.title_wrap {
	position: relative;
	width: 150%;
	height: 250rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.title_wrap image {
	width: 100%;
	height: 100%;
	margin-left: 30rpx;
}

