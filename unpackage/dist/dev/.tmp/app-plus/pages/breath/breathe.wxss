
.container {
	position: absolute;
	width: 100%;
	height: 100%;
}
.container image {
	position: absolute;
	width: 100%;
	height: 100%;
}
.title {
	position: relative;
	margin-top: 100rpx;
	margin-left: 50rpx;
	width: 150rpx;
	height: 100rpx;
}
@-webkit-keyframes circlechangge {
0% {
		-webkit-transform: scale(0.6);
}
30%,
	60% {
		-webkit-transform: scale(1);
}
100% {
		-webkit-transform: scale(0.6);
}
}
@keyframes circlechangge {
0% {
		-webkit-transform: scale(0.6);
}
30%,
	60% {
		-webkit-transform: scale(1);
}
100% {
		-webkit-transform: scale(0.6);
}
}
.lopp {
	position: relative;
	width: 700rpx;
	height: 700rpx;
	margin-left: 25rpx;
	-webkit-animation: circlechangge 10s infinite ;
	        animation: circlechangge 10s infinite ;
}
@-webkit-keyframes fontchanghe {
0% {
		-webkit-transform: scale(0.6);
}
30%,
	60% {
		-webkit-transform: scale(1);
}
100% {
		-webkit-transform: scale(0.6);
}
}
@keyframes fontchanghe {
0% {
		-webkit-transform: scale(0.6);
}
30%,
	60% {
		-webkit-transform: scale(1);
}
100% {
		-webkit-transform: scale(0.6);
}
}
.textsize {
	position: relative;
	width: 750rpx;
	height: 50rpx;
	text-align: center;
	margin-top: -360rpx;
	font-family: PingFangSC-Thin, sans-serif;
	color: #ffffff;
	-webkit-animation: fontchanghe 10s infinite ;
	        animation: fontchanghe 10s infinite ;
}
